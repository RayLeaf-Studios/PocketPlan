    <style name="MyDatePickerDialogTheme" parent="android:Theme.Material.Dialog">
        <item name="android:datePickerStyle">@style/MyDatePickerStyle</item>
    </style>

    <style name="MyDatePickerStyle" parent="android:Widget.Material.DatePicker">
        <item name="android:headerBackground">?attr/colorNotePurple</item>
        <item name="android:windowIsFloating">true</item>
        <item name="colorBackgroundFloating">?attr/colorAccent</item>
        <item name="android:dayOfWeekBackground">?attr/colorAccent</item>
        <item name="android:dayOfWeekTextAppearance">?attr/colorAccent</item>
        <item name="background">?attr/colorBackground</item>
    </style>

    <style name="MyTimePickerDialogTheme" parent="android:Theme.Material.Dialog">
        <item name="android:timePickerStyle">@style/MyTimePickerStyle</item>
    </style>

    <style name="MyTimePickerStyle" parent="android:Widget.Material.TimePicker">
        <item name="android:headerBackground">?attr/colorAccent</item>
        <item name="android:numbersSelectorColor">?attr/colorAccent</item>
    </style>

    <style name="DialogTheme" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorAccent">?attr/colorAccentClone</item>
        <item name="colorControlNormal">?attr/colorOnBackGround</item>
    </style>

    <style name="CheckStyleTask">
        <item name="colorAccent">?attr/colorHint</item>
        <item name="android:colorControlNormal">?attr/colorOnBackGroundTask</item>
    </style>

    <style name="CheckStyleItem">
        <item name="colorAccent">?attr/colorHint</item>
    </style>

    <style name="MyLightToolbarStyle" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:colorBackground">?attr/colorBackground</item>
        <item name="android:textColor">?attr/colorOnBackGround</item>
    </style>

    <style name="mySpinnerItemStyleLight" parent="@android:style/Widget.Holo.DropDownItem.Spinner">
